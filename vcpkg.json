{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "krkr2",
  "version-string": "1.4.0",
  "dependencies": [
    "boost-iostreams",
    "boost-phoenix",
    "boost-spirit",
    {
      "name": "breakpad",
      "default-features": false
    },
    "cocos2dx",
    "ffmpeg",
    "jxrlib",
    "libarchive",
    "libbpg",
    "libgdiplus",
    "libjpeg-turbo",
    "libogg",
    "libvorbis",
    "lz4",
    "oboe",
    "oniguruma",
    "openal-soft",
    {
      "name": "opencv4",
      "default-features": false,
      "features": [
        "openmp",
        "freetype",
        "jpeg"
      ]
    },
    "opus",
    {
      "name": "opusfile",
      "default-features": false
    },
    "p7zip",
    {
      "name": "sdl2",
      "default-features": false
    },
    "unrar"
  ],
  "overrides": [
    {
      "name": "opencv4",
      "version": "4.7.0#6"
    }
  ]
}
