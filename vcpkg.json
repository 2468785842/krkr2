{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "krkr2",
  "version-string": "1.5.0",
  "dependencies": [
    "7zip",
    {
      "name": "argparse",
      "platform": "linux | windows | osx"
    },
    "boost-iostreams",
    {
      "name": "boost-locale",
      "default-features": false
    },
    "boost-phoenix",
    "boost-spirit",
    {
      "name": "breakpad",
      "platform": "android",
      "default-features": false
    },
    {
      "name": "catch2",
      "platform": "!android & !ios",
      "default-features": false
    },
    "cocos2dx",
    {
      "name": "dirent",
      "platform": "windows"
    },
    "ffmpeg",
    "fmt",
    "jxrlib",
    {
      "name": "glfw3",
      "platform": "linux | windows | osx"
    },
    "libarchive",
    {
      "name": "libgdiplus",
      "platform": "!windows"
    },
    "libjpeg-turbo",
    {
      "name": "libogg",
      "platform": "android"
    },
    {
      "name": "libpng",
      "platform": "!linux"
    },
    "libvorbis",
    "lz4",
    "oniguruma",
    "openal-soft",
    "libwebp",
    {
      "name": "opencv4",
      "default-features": false,
      "features": [
        "openmp"
      ]
    },
    {
      "name": "opus",
      "platform": "android"
    },
    {
      "name": "opusfile",
      "default-features": false
    },
    {
      "name": "sdl2",
      "default-features": false
    },
    "spdlog",
    "uchardet",
    "unrar",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    "zstd"
  ],
  "overrides": [
    {
      "name": "opencv4",
      "version": "4.7.0#6"
    }
  ]
}
